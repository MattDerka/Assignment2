CREATE(5, 10, 25, 100; 3; 10; 10; 10)
CONFIGURE([0] "Coke", 200; "water", 250; "stuff", 205)
POP_LOAD([0] 0; "Coke", 1)
POP_LOAD([0] 1; "water", 1)
POP_LOAD([0] 2; "stuff", 1)
PRESS([0] 0)
EXTRACT([0])
CHECK_DELIVERY(0)
INSERT([0] 100)
INSERT([0] 100)
INSERT([0] 100)
PRESS([0] 0)
EXTRACT([0])
CHECK_DELIVERY(100, "Coke")
UNLOAD([0])
CHECK_TEARDOWN(200; 0; "water", "stuff")
